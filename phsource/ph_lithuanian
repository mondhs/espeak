
phoneme @
  vowel starttype #@ endtype #@
  length 120
  FMT(vowel/8_7)
endphoneme

phoneme E
  vowel starttype #e endtype #e
  length 180
  IF nextPh(isPause) THEN
    FMT(vowel/ee_1)
  ENDIF
  FMT(vowel/e_mid)
endphoneme


phoneme i
  vowel starttype #i endtype #i
  length 100
  IfNextVowelAppend(;)
  FMT(vowel/ii_3)
endphoneme
 

phoneme ai
  vowel starttype #a endtype #i
  length 310
  long
  FMT(vdiph/aai)
endphoneme

