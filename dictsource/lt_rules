     c) ia (cijL13_    ija // J tariamos nors nerašomas: diferenciacija
     c) ia (lL12_      ija // J tariamos nors nerašomas: diferencialas
        ie         ;ie
     _) ie (v      jie // Ieva [Jieva]
        ie (šk     jie // ieškoti [jieškoti]
     _) ie (šm     jie // iešmas [jiešmas]
