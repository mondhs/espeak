     a) i (kL13_   ji // J tariamos nors nerašomas: archaika
     a) i (zmL12_  ji // J tariamos nors nerašomas: archaizmas
        i (A       ;  // jei po to eina balsis – interpretuoti kaip minkštumo ženklą
     
